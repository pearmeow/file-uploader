datasource db {
    url      = env("DATABASE_URL")
    provider = "postgresql"
}

generator client {
    provider = "prisma-client-js"
    output   = "../src/generated/prisma"
}

model Test {
    id Int @id @default(autoincrement())
}

model Session {
    id        String   @id
    sid       String   @unique
    data      String
    expiresAt DateTime
}
